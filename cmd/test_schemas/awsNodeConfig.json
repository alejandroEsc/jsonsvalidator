{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://judkins.house/apis/k2/v1/awsNodeConfig.json",
  "$$target": "awsNodeConfig.json",
  "title": "AWS Node Configuration",
  "description": "Simplified, version of the original kraken config.",

  "properties": {
    "name": {
      "description": "Name of node for use by k2.",
      "type": "string"
    },
    "kind": {
      "description": "Type of config this is.",
      "enum": [ "node" ],
      "type": "string"
    }
  },
  "required": [
    "name"
  ],

  "type": "object"
}